{
  "expo": {
    "name": "",
    "slug": "pokemon-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/novo.png",
    "scheme": "pokemonapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "",
        "backgroundImage": "",
        "monochromeImage": ""
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
  "expo-splash-screen",
    {
    "image": "./assets/images/splash.png",
    "backgroundColor": "#232323",
    "resizeMode": "contain",
    "height": 400,
    
    "dark": {
      "image": "./assets/images/splash.png",
      "backgroundColor": "#000000"
    },
    "imageWidth": 200,
    "imageHeight": 200,
    "color": "black"
    }
  ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
